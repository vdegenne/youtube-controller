{
  "manifest_version": 3,
  "name": "youtube-controller",
  "description": "",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [ "https://www.youtube.com/*" ],
      "js": [ "inject.js" ],
      "type": "module"
    }
  ],
  "web_accessible_resources" : [
    {
      "resources": ["content.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}
